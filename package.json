{
  "name": "reactwebpack",
  "type": "module",
  "productName": "Castor",
  "version": "1.0.0",
  "description": "Management and Finance system",
  "author": "Leone de Souza <lleonesouza@live.com>",
  "homepage": "./",
  "main": "electron.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "dev": "webpack --mode development --config webpack.config.js --watch",
    "build": "webpack --mode production",
    "e-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "electron": "electron .",
    "ofuscate": "",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron2 --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron2 --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^7.5.0",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "accesscontrol": "^2.2.1",
    "babel-loader": "^8.0.6",
    "bluebird": "^3.7.1",
    "cron": "^1.7.2",
    "electron-json-storage": "^4.1.8",
    "electron-store": "^5.1.0",
    "emotion-theming": "^10.0.19",
    "is-electron": "^2.2.0",
    "month-days": "^3.0.0",
    "node-command-line": "^1.0.2",
    "react": "^0.0.0-experimental-b53ea6ca0",
    "react-dom": "^0.0.0-experimental-b53ea6ca0",
    "react-filter-search": "^1.0.8",
    "react-i18next": "^11.1.0",
    "react-lottie": "^1.2.3",
    "react-router-dom": "^5.1.2",
    "react-search": "^2.1.1",
    "react-search-input": "^0.11.3",
    "react-spring": "^8.0.27",
    "type-check": "^0.3.2",
    "xlsx": "^0.15.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "electron": "^7.1.1",
    "electron-reload": "^1.5.0",
    "enzyme": "^3.10.0",
    "html-webpack-plugin": "^3.2.0",
